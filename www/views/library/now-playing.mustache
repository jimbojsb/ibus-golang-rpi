<div class="col-xs-12" id="jukebox-playlists-all">
    <h1>Now Playing</h1>
    <!--<button class="btn btn-info"><i class="fa fa-random"></i> Shuffle Songs</button>-->
    <table class="table table-striped">
        {{#songs}}
            <tr id="song-">
                <td class="song-name">{{Artist}} - {{Title}}</td>
                <td class="song-length">{{Length}}</td>
            </tr>
        {{/songs}}
    </table>
</div>